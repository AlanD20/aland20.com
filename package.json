{
  "author": "AlanD20",
  "name": "aland20-portfolio",
  "description": "Dyanamic portfolio",
  "license": "GPL",
  "version": "2.2.3",
  "private": true,
  "scripts": {
    "dev": "yarn css:prod && next dev",
    "build": "yarn css:prod && next build",
    "start": "next start",
    "lint": "next lint",
    "eslint": "eslint pages src",
    "format": "prettier --write pages src",
    "format:export": "prettier --write prisma/export",
    "css:dev": "postcss ./src/css --dir ./styles --watch",
    "css:prod": "postcss ./src/css --dir ./styles",
    "prisma:studio": "prisma studio",
    "db:push": "prisma db push --force-reset && prisma generate",
    "db:seed": "prisma db seed",
    "db:reset": "yarn db:push && yarn db:seed",
    "generate:secret": "openssl rand -base64 32",
    "pm2": "pm2 start pm2.json"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.3",
    "@prisma/client": "^4.0.0",
    "@reduxjs/toolkit": "^1.8.1",
    "axios": "^0.27.2",
    "cors": "^2.8.5",
    "html-react-parser": "^1.4.14",
    "lodash": "^4.17.21",
    "moment": "^2.29.3",
    "next": "^12.2.0",
    "next-auth": "^4.3.4",
    "next-connect": "^0.12.2",
    "nodemailer": "^6.7.5",
    "prisma": "^4.0.0",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-redux": "^8.0.1",
    "react-responsive": "^9.0.0-beta.8",
    "sharp": "^0.30.6",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/node": "^17.0.37",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.4",
    "@types/react-redux": "^7.1.24",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "autoprefixer": "^10.4.7",
    "cssnano": "^5.1.9",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-next": "^12.2.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "postcss": "^8.4.14",
    "postcss-cli": "^10.0.0",
    "prettier": "^2.6.2",
    "tailwindcss": "^3.0.24",
    "ts-node": "^10.8.0",
    "typescript": "^4.7.2"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  }
}
