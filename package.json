{
  "author": "AlanD20",
  "name": "aland20-portfolio",
  "description": "Dyanamic portfolio",
  "license": "GPL",
  "version": "2.2.3",
  "private": true,
  "scripts": {
    "dev": "yarn css:prod && next dev",
    "build": "yarn css:prod && next build",
    "start": "next start",
    "lint": "next lint",
    "eslint": "eslint pages src",
    "format": "prettier --write pages src",
    "format:export": "prettier --write prisma/export",
    "css:dev": "postcss ./src/css --dir ./styles --watch",
    "css:prod": "postcss ./src/css --dir ./styles",
    "prisma:studio": "prisma studio",
    "db:push": "prisma db push --force-reset && prisma generate",
    "db:seed": "prisma db seed",
    "db:reset": "yarn db:push && yarn db:seed",
    "generate:secret": "openssl rand -base64 32",
    "pm2": "pm2 start pm2.json",
    "deploy": "yarn db:reset && yarn build"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.5",
    "@prisma/client": "^4.12.0",
    "@reduxjs/toolkit": "^1.8.1",
    "axios": "^1.3.5",
    "cors": "^2.8.5",
    "html-react-parser": "^3.0.15",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "next": "^13.3.0",
    "next-auth": "^4.22.0",
    "next-connect": "^0.13.0",
    "nodemailer": "^6.9.1",
    "prisma": "^4.12.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "^8.0.1",
    "react-responsive": "^9.0.2",
    "sharp": "^0.32.0",
    "yup": "^1.1.0"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@types/react": "18.0.35",
    "@types/react-dom": "18.0.11",
    "@types/react-redux": "^7.1.24",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "autoprefixer": "^10.4.14",
    "cssnano": "^6.0.0",
    "eslint": "^8.38.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-next": "^13.3.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "prettier": "^2.8.7",
    "tailwindcss": "^3.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  }
}